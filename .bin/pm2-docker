#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lorenzorasmussen/.local/share/mcp-superassistant/node_modules/.pnpm/pm2@6.0.13/node_modules/pm2/bin/node_modules:/Users/lorenzorasmussen/.local/share/mcp-superassistant/node_modules/.pnpm/pm2@6.0.13/node_modules/pm2/node_modules:/Users/lorenzorasmussen/.local/share/mcp-superassistant/node_modules/.pnpm/pm2@6.0.13/node_modules:/Users/lorenzorasmussen/.local/share/mcp-superassistant/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lorenzorasmussen/.local/share/mcp-superassistant/node_modules/.pnpm/pm2@6.0.13/node_modules/pm2/bin/node_modules:/Users/lorenzorasmussen/.local/share/mcp-superassistant/node_modules/.pnpm/pm2@6.0.13/node_modules/pm2/node_modules:/Users/lorenzorasmussen/.local/share/mcp-superassistant/node_modules/.pnpm/pm2@6.0.13/node_modules:/Users/lorenzorasmussen/.local/share/mcp-superassistant/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/pm2@6.0.13/node_modules/pm2/bin/pm2-docker" "$@"
else
  exec node  "$basedir/../.pnpm/pm2@6.0.13/node_modules/pm2/bin/pm2-docker" "$@"
fi
